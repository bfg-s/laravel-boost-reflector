{
    "name": "bfg/laravel-boost-reflector",
    "type": "library",
    "description": "Laravel boost with reflection of classes",
    "keywords": [
        "php",
        "laravel",
        "template"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bfg\\LaravelBoostReflector\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bfg\\LaravelBoostReflector\\LaravelBoostReflectorServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Xsaven",
            "email": "xsaven@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "scripts": {

    },
    "require": {
        "php": "^8.1|^8.2",
        "laravel/boost": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.6",
        "bfg/attributes": "^1.2",
        "roave/better-reflection": "^6.59"
    },

    "version": "v1.0.0"
}
